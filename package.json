{
  "version": "0.0.0-development",
  "name": "eslint-config-dacz",
  "description": "Shareable eslint config for dacz",
  "author": "David Cizek <david@origamica.com> (https://twitter.com/dadc)",
  "homepage": "https://github.com/dacz/eslint-config-dacz",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dacz/eslint-config-dacz.git"
  },
  "bugs": {
    "url": "https://github.com/dacz/eslint-config-dacz/issues"
  },
  "readme": "README.md",
  "directories": {},
  "keywords": [
    "eslint",
    "prettier",
    "codestyle"
  ],
  "scripts": {
    "cm": "git-cz",
    "cm:retry": "git-cz --retry",
    "lint": "eslint .",
    "test": "npm run --silent lint",
    "eslint-check": "eslint --print-config index.js | eslint-config-prettier-check",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-plugin-ava": "^4.4.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "prettier": "^1.10.2",
    "semantic-release": "^12.2.2",
    "travis-deploy-once": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
